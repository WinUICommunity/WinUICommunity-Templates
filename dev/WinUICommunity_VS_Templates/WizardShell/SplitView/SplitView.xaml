                                                      From="ClosedCompactRight"
                                                      To="OpenCompactOverlayRight" />
                                    <VisualTransition Storyboard="{StaticResource FromOpenOverlayLeftToClosed}"
                                                      From="OpenOverlayLeft"
                                                      To="Closed" />
                                    <VisualTransition Storyboard="{StaticResource FromOpenOverlayRightToClosed}"
                                                      From="OpenOverlayRight"
                                                      To="Closed" />
